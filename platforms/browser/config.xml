<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <icon src="www/img/icon.png" />
    <preference name="SplashScreen" value="img/splashscreen.gif" />
    <preference name="SplashScreenDelay" value="3500" />
    <preference name="SplashScreenBackgroundColor" value="black" />
    <preference name="SplashScreenWidth" value="600" />
    <preference name="SplashScreenHeight" value="300" />
    <name>List Of Movies</name>
    <description>
        An application to test Apache Cordova with the API Rotten Tomatoes.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Nicolas Heldmaier and AnaÃ«lle Hamon
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
